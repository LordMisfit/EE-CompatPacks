			}
			LMWZ E 1
			{
				if (exex_monsterlevelenabledflags && CountInv("MonsterLevel") > 240) { A_SetTics(0); }
				A_StartSound("wizard/tele");
			}
			goto See

		Pain:
			LMWZ I 3
			LMWZ I 5 A_EEPain
			LMWZ I 0 A_StartSound("wizard/tele")
			Goto See
		Pain.Eldritch:
		Pain.Holy:
			LMWZ I 3
			LMWZ I 5 A_EEPain
			LMWZ IIIIIIII 3
			LMWZ I 0 A_StartSound("wizard/tele")
			Goto See

		Death:
