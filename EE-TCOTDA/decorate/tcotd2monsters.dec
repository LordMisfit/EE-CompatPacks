				if (random(1,256) <= (8 + (CountInv("MonsterLevel") / 11.25)) && specialspeed1 <= 0) { specialspeed1 = ((randompick(6,8,8,8,10) + (CountInv("MonsterLevel") / frandompick(2.5,5,5,7.5,7.5,7.5))) * 1.5); }
			}
			N00B A 0 
			{
				user_avoidmeleetimer++;
