			{
				if (exex_monsterlevelenabledflags && CountInv("MonsterLevel") > 60) { A_SetTics(2); }
				A_FaceTarget;
			}
			N00B III 3 
			{
				if (exex_monsterlevelenabledflags && CountInv("MonsterLevel") > 60) { A_SetTics(2); }

				A_CustomComboAttack("ClayDevilMissile", 32, (frandom(2.4,4.8)*random(1,8)), "imp/melee");
